extends layouts/main 
include mixins/videoBlock

block contents
    p videoSearch!
    .search__header 
        h3 Searching for: #{searchingBy}
    .search__video 
        each item in Videos 
            +videoBlock({
                videoFile: item.videoFile,
                title: item.title,
                views: item.views,
                id: item.id
            })
    
